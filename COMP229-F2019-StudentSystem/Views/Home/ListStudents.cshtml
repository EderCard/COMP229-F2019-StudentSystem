@model IEnumerable<Student>
@{
    ViewBag.Title = "List of Students";

}

    <table class="table">
        <thead>
            <tr>
                <td>Student Id</td>
                <td>First Name</td>
                <td>Last Name</td>
                <td>Email</td>
                <td>Course</td>
                <td>FullTime</td>
                <td>Gpa</td>
            </tr>
        </thead>

        <tbody>
            @foreach (var s in Model)
             {
                 @await Html.PartialAsync("StudentSummary", s)
             }
        </tbody>

    </table>

